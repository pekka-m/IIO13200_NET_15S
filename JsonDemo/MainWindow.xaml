<Window x:Name="JsonDemo" x:Class="JsonDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JsonDemo"
        mc:Ignorable="d"
        Title="JsonDemo" Height="325.491" Width="525.001">
    <Grid>
        <Button x:Name="button" Content="Get Json" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Stretch" Margin="10,35,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="72"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Stretch" Margin="10,112,10,10" VerticalAlignment="Stretch" ItemsSource="{Binding}"/>
    </Grid>
</Window>
